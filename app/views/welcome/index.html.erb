<script> var currentUserId = <%= current_user.id %></script>

<script type="text/template" id="ranking-list-template">
  <%% collection.each(function(movie){  %>
    <li alt="Movie title"><%%= movie.get('title') %></li>
  <%% }) %>
</script>

<script type="text/template" id="movie-comparison-template">
  <%% collection.each(function(movie){  %>
    <div class="col-md-6">
    <img data-id="<%%= movie.get('id') %>" src="<%%= movie.get('poster_url') %>" class="img-rounded img-responsive movie-choice" alt="<%%= movie.get('title') %>">
    <figcaption><strong><%%= movie.get('title') + " - " + movie.get('year') %></strong></figcaption>
    </div>
  <%% }) %>
</script>
